    <validationRules>
        <fullName>Opp_Must_be_Lost_for_Competitor_Won</fullName>
        <active>true</active>
        <description>If the Status of the Competitor is Won, then the Opportunity Stage must be closed lost.  Error appears at the top of the page</description>
        <errorConditionFormula>AND(
Won__c = True,
NOT(ISPICKVAL(Opportunity__r.StageName, &quot;Closed Lost&quot;)))</errorConditionFormula>
        <errorMessage>Only Lost Opportunities can have a Competitor that won</errorMessage>
    </validationRules>
